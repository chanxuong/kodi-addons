<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.phimonline"
       name="PhimOnline"
       version="1.0.0"
       provider-name="PhimOnline">
    <requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="script.module.simple.downloader" version="1.9.4" />
		<import addon="script.module.beautifulsoup" version="3.0.8" />
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <language>en</language>
        <summary lang="en">PhimOnline</summary>
        <description lang="en">PhimOnline</description>
    </extension>
</addon>

<addon id="repository.chanxuong" name="Chan Xuong Repo" version="1.1" provider-name="Chan Xuong Repo">
	<extension point="xbmc.addon.repository" name="Chan Xuong Repo">
		<info compressed="false">http://raw.github.com/chanxuong/kodi-addons/master/addons.xml</info>
		<checksum>http://raw.github.com/chanxuong/kodi-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/chanxuong/kodi-addons/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Chan Xuong Repo</summary>
		<description></description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
